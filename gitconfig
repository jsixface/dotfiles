[difftool]
	config = false
	prompt = false
[alias]
	d = difftool
	c = "commit -a"
	cm = "commit -am"
	st = "status -bs"
	lg = log --oneline --graph -20
	showtrackedignored = ls-files -i --exclude-standard
[push]
	default = simple
[credential]
	helper = store
[core]
	quotepath = false
[url "git@gitlab.com:"]
	insteadOf = https://gitlab.com/
[diff]
	tool = vimdiff
