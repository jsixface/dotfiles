[difftool]
	config = false
	prompt = false
[alias]
	d = difftool
	c = "commit -a"
	cm = "commit -am"
	st = "status -bs"
	lg = log --oneline --graph -20
	showtrackedignored = ls-files -i --exclude-standard
[push]
	default = simple
[core]
	quotepath = false
	eol = lf
	autocrlf = input
[diff]
	tool = vimdiff
[pull]
	ff = only
	rebase = true
[safe]
	directory = /home/sixface/git/mc
[init]
	defaultBranch = master
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true

[gpg]
	format = ssh
[pager]
	branch = false
	config = false
[user]
	name = Arumugam Jeganathan
