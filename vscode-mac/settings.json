// Place your settings in this file to overwrite the default settings
{
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "atlascode.bitbucket.enabled": true,
  "atlascode.jira.jqlList": [
    {
      "enabled": true,
      "id": "855f471a-5cdc-4508-8e44-25c226dd4603",
      "monitor": true,
      "name": "My getbevi Issues",
      "query": "assignee = currentUser() AND resolution = Unresolved ORDER BY lastViewed DESC",
      "siteId": "120f530b-e4f0-42c4-8a7f-078804a29f5a"
    }
  ],
  "atlascode.jira.lastCreateSiteAndProject": {
    "projectKey": "EN",
    "siteId": "120f530b-e4f0-42c4-8a7f-078804a29f5a"
  },
"debug.console.fontSize": 16,
  "diffEditor.renderSideBySide": true,
  "editor.fontFamily": "'JetBrains Mono Medium', monospace",
  "editor.fontSize": 14,
  "editor.rulers": [
    100
  ],
  "editor.wordWrap": "on",
  "editor.fontLigatures": true,
  "extensions.ignoreRecommendations": true,
  "files.eol": "\n",
  "files.exclude": {
    "**/._sync_*": true,
    "**/.sync_*": true
  },
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "go.useLanguageServer": true,
  "jupyter.appendResults": true,
  "python.autoComplete.addBrackets": true,
  "python.autoComplete.extraPaths": [
    "${workspaceRoot}/lib/python3.6/site-packages/"
  ],
  "python.autoComplete.preloadModules": [
    "numpy",
    "pandas",
    "matplotlib",
    "sklearn"
  ],
  "python.formatting.provider": "yapf",
  "python.jediEnabled": false,
  "python.languageServer": "Default",
  "python.pythonPath": "/bin/python",
  "python.workspaceSymbols.exclusionPatterns": [],
  "redhat.telemetry.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.fontFamily": "Roboto Mono for Powerline",
  "terminal.integrated.fontSize": 16,
  "update.mode": "manual",
  "window.titleBarStyle": "custom",
  "workbench.colorCustomizations": {
    "editor.selectionBackground": "#666"
  },
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.iconTheme": "Monokai Pro (Filter Spectrum) Monochrome Icons",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.tree.indent": 4,
  "yaml.schemas": {
    "file:///home/sixface/.vscode/extensions/atlassian.atlascode-2.8.6/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "zenMode.hideTabs": false,
  "python.analysis.extraPaths": [
    "${workspaceRoot}/lib/python3.6/site-packages/"
  ],
  "python.analysis.completeFunctionParens": true,
  "workbench.colorTheme": "Monokai Dimmed"
}